{
   "background": {
    	"persistent": false,
    	"scripts": [ "backgroundWorker.js" ]
   },
   "content_scripts": [ {
    	"js": [ "addService.js" ],
    	"matches": [ "https://www.fimfiction.net/story/*" ]
   } ],
   "description": "Enables direct conversion and uploading of Scrivener projects to fimfiction.net. (This product is not affiliated with Literature & Latte or fimfiction.net)",
   "key": "",
   "applications": {
   		"gecko": {
			"id": "{cff512eb-a6fd-405d-a0d6-6d4853df2ed7}"
		}
	},
   "manifest_version": 2,
   "name": "Scriv2Fic",
   "icons": {
   		"16": "Icons/scriv2ficIcon16.png",
   		"48": "Icons/scriv2ficIcon48.png",
   		"128": "Icons/scriv2ficIcon128.png"
   },
   "permissions": [ 
		"activeTab",
		"identity",
		"notifications",
		"tabs",
		"https://www.fimfiction.net/*"
   ],
   "version": "1.0"
}